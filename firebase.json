{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "nodejs20",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-hadolint.log"
      ]
    }
  ],
  "extensions": {
    "firestore-stripe-payments": "invertase/firestore-stripe-payments@0.3.12"
  }
}
